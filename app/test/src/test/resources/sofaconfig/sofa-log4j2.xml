<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss,SSS}  %-5p %C{1} : %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="Console"/>
        </Root>
        <Logger name="com.alipay.sofa" level="INFO">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="com.alipay.sofa.runtime.test.pipeline.stage.EclipseModuleLogOutputStage" level="INFO">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="com.alipay.sofa.runtime.test.pipeline.stage.EclipseSpringContextInstallStage" level="INFO">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="com.alipay.cloudengine.kernel.notifier" level="WARN">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="com.alipay.sofa.service.spi.event" level="WARN">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="com.alipay.sofa.service.hsf.service.handler" level="WARN">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="org.springframework.osgi" level="WARN">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="org.springframework.beans.factory" level="WARN">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="com.alipay.cloudengine.kernel.deployer.pipiline.stage" level="WARN">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="java.sql" level="ERROR">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="com.ibatis" level="WARN">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="om.alipay.common.webx.session" level="WARN">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="TBCACHE-DIGEST" level="WARN">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="TAIR-OPERATION-DIGEST" level="WARN">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="TBCACHE-CONNPOOL-DIGEST" level="WARN">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="org.mule" level="WARN">
            <AppenderRef ref="Console"/>
        </Logger>
    </Loggers>
</Configuration>
