<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//iBATIS.com//DTD SQL Map Config 2.0//EN" "http://www.ibatis.com/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
    <settings cacheModelsEnabled="true" enhancementEnabled="false"
		lazyLoadingEnabled="false" maxRequests="3000" maxSessions="3000"
		maxTransactions="3000" useStatementNamespaces="false" />
    
#foreach ($table in $plugin.sortedTables)            
    <sqlMap resource="sqlmap/${plugin.middlegen.middlegenTask.business}/auto/${table.baseClassName}-sqlmap-mapping.xml" />
#end

</sqlMapConfig>
